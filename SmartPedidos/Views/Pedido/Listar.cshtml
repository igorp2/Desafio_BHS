<a type="button" class="btn btn-secondary" href="../">Voltar</a><br><br>

<table class="table table-hover">

    <tr>
        <th>Descrição</th>
        <th>Valor</th>
    </tr>

    @foreach (Item item in ViewBag.Itens)
    {
        //Verificar o Codigo do Pedido para imprimir itens...

        <tr>
            <td>@item.Descricao</td>
            <td>@item.ValorItem</td>
        </tr>

    }

</table>
